# .env file for local development
# IMPORTANT: This file should NOT be committed to version control.
# Add .env to your .gitignore file.

# MySQL Database URL
# Format: mysql+pymysql://user:password@host:port/dbname
DATABASE_URL="mysql+pymysql://root:your_password@127.0.0.1:3306/your_db_name"

# WeChat Mini Program Credentials
WECHAT_APP_ID="your_real_wechat_app_id"
WECHAT_APP_SECRET="your_real_wechat_app_secret"

# You can override other settings here as well
# SECRET_KEY="a_more_secure_random_string"